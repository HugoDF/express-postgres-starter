{
  "name": "express-postgres-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Starter project using Express and Postgres",
  "main": "config.js",
  "scripts": {
    "test": "xo",
    "lint": "xo",
    "format": "xo --fix",
    "start": "node ./bin/start.ts",
    "migrate": "node ./bin/migrate.js",
    "compile": "./node_modules/.bin/tsc-watch --onSuccess \"node ./dist/bin/start.js \"",
    "dev": "nodemon --exec \"npm run compile\"",
    "migrate:create": "migrate create --migrations-dir='./src/migrations'"
  },
  "keywords": [
    "express",
    "postgres"
  ],
  "author": "Hugo Di Francesco",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "client-sessions": "^0.8.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "migrate": "^1.7.0",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "sql-template-strings": "^2.2.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "nodemon": "^2.0.7",
    "tsc-watch": "^4.2.9",
    "xo": "^0.36.1"
  },
  "xo": {
    "prettier": true,
    "space": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoDF/express-postgres-starter.git"
  },
  "bugs": {
    "url": "https://github.com/HugoDF/express-postgres-starter/issues"
  },
  "homepage": "https://github.com/HugoDF/express-postgres-starter#readme"
}
